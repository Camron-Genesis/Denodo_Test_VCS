e ) INNER JOIN iv_validation_address AS iv_validation_address ON (iv_home_addresses.personstreet_0 = iv_validation_address.personstreet AND iv_home_addresses.personzip_0 = iv_validation_address.actualpostalcode AND iv_home_addresses.personzip_0 = iv_validation_address.postalcode) ) INNER JOIN iv_weather_data AS iv_weather_data ON iv_home_addresses.personzip_0 = iv_weather_data.location_0 ;

